{
  "name": "material-ui-audio-player",
  "version": "1.7.1",
  "description": "Audio player for material ui design",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Werter12/material-ui-audio-player.git"
  },
  "engines": {
    "node": ">=v16.17.0",
    "yarn": ">=v1.12.0",
    "npm": ">=8.15.0"
  },
  "author": "Werter12 <samofan77@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint -c .eslintrc.js src/**/*.{ts,tsx} stories/**/*.tsx",
    "fix": "eslint -c .eslintrc.js src/**/*.{ts,tsx} stories/**/*.tsx --fix",
    "test": "jest --maxWorkers=1 --verbose",
    "watch": "jest --watchAll",
    "coverage": "jest --collect-coverage",
    "start": "storybook -p 6006",
    "build": "yarn tsc",
    "deploy-storybook": "storybook-to-ghpages",
    "preversion": "yarn test",
    "postversion": "git push --follow-tags && echo \"Successfully released version $npm_package_version!\""
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^7.0.0",
    "@storybook/addon-viewport": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/theming": "^7.0.6",
    "@types/enzyme": "3.10.13",
    "@types/jest": "29.5.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^43.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.2.0",
    "storybook-addon-specifications": "^2.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.1",
    "classnames": "^2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.js src/**/*.{ts,tsx} stories/**/*.tsx --fix",
      "prettier --write",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/Werter12/material-ui-audio-player/issues"
  },
  "homepage": "https://github.com/Werter12/material-ui-audio-player#readme",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "audio player"
  ]
}
